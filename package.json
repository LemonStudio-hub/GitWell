{
  "name": "gitdash",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "开源项目健康仪表盘",
  "scripts": {
    "dev": "pnpm --filter @gitdash/web dev",
    "build": "pnpm --filter @gitdash/web build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,vue,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,vue,json,md}\"",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.27.0",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.30.1"
}